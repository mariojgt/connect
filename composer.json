{
    "name": "mariojgt/connect",
    "description": "A Laravel Package Connect, a quick start for new laravel Packages",
    "keywords": [
        "laravel"
    ],
    "type": "laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "mario",
            "email": "mariojgt2@gmail.com"
        }
    ],
    "require":{
        "php": "^7.3|^8.0",
        "pusher/pusher-php-server": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Mariojgt\\Connect\\": "src"
        }
    },

    "scripts": {
        "post-autoload-dump": [
            "@php artisan route:list"
        ]
    },

    "extra": {
        "laravel": {
            "providers": [
                "Mariojgt\\Connect\\ConnectProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
